{
  "name": "uber-server",
  "version": "1.0.0",
  "description": "cloning the uber application",
  "main": "index.js",
  "repository": "https://github.com/harrycjy1/uber-clone.git",
  "author": "choddol",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.1",
    "@types/helmet": "^0.0.43",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.13.0",
    "@types/twilio": "^0.0.10",
    "babel-runtime": "^6.26.0",
    "copy": "^0.3.2",
    "gql-merge": "^0.0.6",
    "graphql-to-typescript": "^0.1.2",
    "ts-node": "^8.0.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.2"
  },
  "scripts": {
    "predev": "yarn run types",
    "dev": "cd src && nodemon --exec ts-node index.ts -e ts,graphql",
    "pretypes": "gql-merge --out-file ./src/schema.graphql ./src/api/**/*.graphql",
    "types": "graphql-to-typescript ./src/schema.graphql ./src/types/graph.d.ts",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "postbuild": "cd src && copy ./api/**/*.graphql ../dist/api",
    "start": "cd dist && node ."
  },
  "dependencies": {
    "@types/mailgun-js": "^0.16.3",
    "bcrypt": "^3.0.5",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "graphql-tools": "^4.0.4",
    "graphql-yoga": "^1.17.4",
    "helmet": "^3.16.0",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "merge-graphql-schemas": "^1.5.8",
    "morgan": "^1.9.1",
    "pg": "^7.9.0",
    "twilio": "^3.30.1",
    "typeorm": "^0.2.16"
  }
}
